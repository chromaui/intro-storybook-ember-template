{
  "name": "intro-storybook-ember-template",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/chromaui/intro-storybook-ember-template"
  },
  "bugs": {
    "url": "https://github.com/chromaui/intro-storybook-ember-template/issues"
  },
  "license": "MIT",
  "description": "Starter template to get up and running quickly with Ember and Storybook",
  "author": "Chromatic <https://chromatic.com/>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "storybook": "start-storybook -p 6006 -s dist --ci",
    "storybook-dev": "npm-run-all --continue-on-error --parallel start storybook",
    "prebuild-storybook": "ember build",
    "build-storybook": "build-storybook -s dist"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/addon-links": "^6.1.15",
    "@storybook/ember": "^6.1.15",
    "@storybook/ember-cli-storybook": "^0.3.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-ember-modules-api-polyfill": "^3.2.1",
    "babel-plugin-htmlbars-inline-precompile": "^4.2.1",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.8.0",
    "ember-cli": "~3.23.0",
    "ember-cli-app-version": "^4.0.0",
    "ember-cli-babel": "^7.23.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-data": "~3.23.0",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.23.1",
    "ember-template-lint": "^2.14.0",
    "ember-truth-helpers": "^3.0.0",
    "ember-welcome-page": "^4.0.0",
    "eslint": "^7.15.0",
    "eslint-plugin-ember": "^10.0.2",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.6.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "dependencies": {}
}
